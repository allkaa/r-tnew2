{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
          "type": "node",
          "request": "launch",
          "name": "Launch Project",
          "program": "${workspaceFolder}/EventEmitterSample.js",
          "cwd": "${workspaceFolder}"
    },
    {
          "type": "node",
          "request":"launch",
          "name": "Launch Node File",
          "program": "${file}",
          "cwd":"${workspaceFolder}",
    },
    {
          "type": "go",
          "request":"launch",
          "name": "Launch GoLang File",
          "mode": "auto",
          "program": "${fileDirname}",
          "env": {},
          "args": []
    },
    {
        "type": "node",
        "request":"launch",
        "name": "Launch Node --experimental-modules File",
        "program": "${file}",
        "cwd":"${workspaceFolder}",
        "runtimeArgs": ["--experimental-modules"],
    },
    // To use VS debugger with Chrome first start developer server: npm start
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "Chrome localhost:3000",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src",
        "sourceMapPathOverrides": {"webpack:///src/*": "${webRoot}/*"},
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "Chrome localhost:5000",
        "url": "http://localhost:5000",
        "webRoot": "${workspaceFolder}/build",
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "Chrome unl.test:8080",
        "url": "http://unl.test:8080",
        "webRoot": "${workspaceFolder}",
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome ./index.html",
        "file": "${workspaceFolder}/index.html"
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome ./build/index.html",
        "file": "${workspaceFolder}/build/index.html"
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "StyleSheetsDemo.html",
        "file": "${workspaceFolder}/StyleSheetsDemo.html"
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "StyleSheetsDemoGood.html",
        "file": "${workspaceFolder}/StyleSheetsDemoGood.html"
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "indexForm.html",
        "file": "${workspaceFolder}/indexForm.html"
    }
  ]
}